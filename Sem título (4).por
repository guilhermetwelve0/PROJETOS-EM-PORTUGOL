programa {
	funcao inicio() {
       inteiro numero
     
       
       escreva("Digite um numero: \n")
       leia(numero)
       
       se(numero > 0){
           escreva("O numero e positivo")
       }se(numero < 0){
           escreva("O numero e negativo")
       }senao{
           escreva("O numero e nulo")
       }
       
      
     
       
       
	}
}
